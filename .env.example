# Copy to .env and fill values for local development

# Supabase
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_KEY=your_anon_or_service_role_key

# Discord
DISCORD_BOT_TOKEN=your_discord_bot_token

# Health check server (optional in local dev)
# If port 8000 is already in use, change HEALTH_PORT or disable it.
ENABLE_HEALTH_SERVER=1
HEALTH_HOST=0.0.0.0
HEALTH_PORT=8000

